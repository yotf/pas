{
  "name": "aps",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:test": "env-cmd -f .env.test npm run-script start",
    "start:production": "env-cmd -f .env.production npm run-script start",
    "build": "tsc && vite build",
    "build:test": "tsc && NODE_ENV=production vite build --mode test",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest watch",
    "test:noWatch": "vitest run",
    "test:report": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:report": "npm run lint -- -f json -o eslint-report.json",
    "typescript": "tsc",
    "documentation": "typedoc",
    "commit": "cz",
    "prepare": "husky install",
    "release": "standard-version",
    "release:script": "bash ./scripts/release.sh"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/manrope": "^4.5.12",
    "@hookform/resolvers": "^2.9.10",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "antd": "^5.1.1",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "i18next": "^22.0.2",
    "jwt-decode": "^3.1.2",
    "rc-picker": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.39.1",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sass": "^1.55.0",
    "typedoc-plugin-rename-defaults": "^0.6.5",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.5",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-typescript": "^0.53.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sonar-scanner": "^3.1.0",
    "standard-version": "^9.5.0",
    "typedoc": "^0.23.19",
    "typedoc-theme-hierarchy": "^3.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.0",
    "vitest-sonar-reporter": "^0.3.2"
  },
  "peerDependencies": {
    "vitest": "^0.24.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
